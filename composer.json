{
    "name": "nqxcode/laravel-lucene-search",
    "description": "Laravel 4 package for full-text search over Eloquent models based on ZF2 Lucene.",
    "keywords" :["laravel", "full-text", "search", "lucene", "eloquent", "orm" ],
    "authors": [
        {
            "name": "Andrey Pokoev",
            "email": "nc101ux@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "nqxcode/zendsearch": "dev-master",
        "nqxcode/lucene-stemmer-en-ru": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "0.9.*",
        "orchestra/testbench": "2.2.*"
    },
    "autoload": {
        "files": [
            "src/Nqxcode/LaravelSearch/helpers.php"
        ],
        "psr-4": {
            "Nqxcode\\LaravelSearch\\": "src/Nqxcode/LaravelSearch",
            "tests\\": "tests/"
        }
    },

    "prefer-stable" : true,
    "minimum-stability": "dev"
}
